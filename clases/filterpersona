class Persona:
    def __init__(self, nombre, edad):
       self.nombre = nombre
       self.edad = edad
    def __str__(self):
       return "{} de {} aÃ±os".format(self.nombre, self.edad)

personas = [
  Persona("Leonardo", 38),
  Persona("Ana", 33),
  Persona("Sabrina", 12),
  Persona("Enrique", 3)
  ]
menores = filter(lambda persona: persona.edad < 18, personas)
for menor in menores:
      print(menor)
