#program to find biggest of two numbers using functions. (nested if)
def biggest(a,b,c):
    if a>b :
      if a>c :
        return a
      else :
        return c
    else :
       if b>c :
         return b
       else :
         return c
         
a=int(input("Enter a value"))
b=int(input("Enter b value"))
c=int(input("Enter c value"))
#function call
big= biggest(a,b,c)
print("big number= ",big)
